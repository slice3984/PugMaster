{
  "name": "pickup-bot",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:www": "cross-env NODE_ENV=production webpack --config webpack.www.config.js --mode=production",
    "dev:www": "cross-env NODE_ENV=development webpack --config webpack.www.config.js --watch",
    "build:bot_old": "cross-env NODE_ENV=production webpack --config webpack.bot.config.js --mode=production",
    "build:bot": "tsc -p ./tsconfig.json",
    "debug:bot": "cross-env TS_NODE_PROJECT=tsconfig.json DEV=true node --inspect=5858 -r ts-node/register ./app.ts",
    "dev:bot": "cross-env DEBUG=true nodemon",
    "dev:all": "npm-run-all --parallel dev:bot dev:www",
    "build:all": "npm-run-all --parallel build:bot build:www"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "www"
    ],
    "watch": [
      "."
    ],
    "exec": "npm run debug:bot",
    "ext": "ts",
    "stdin": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slice3984/pickup-bot.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slice3984/pickup-bot/issues"
  },
  "homepage": "https://github.com/slice3984/pickup-bot#readme",
  "devDependencies": {
    "@types/chart.js": "^2.9.25",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.11.2",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.34",
    "autoprefixer": "^9.8.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^3.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-livereload-plugin": "^2.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "discord-oauth2": "^2.5.0",
    "discord.js": "^12.3.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.4",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "ffmpeg-static": "^4.2.7",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.2.0",
    "mysql2": "^2.2.5",
    "socket.io": "^2.4.0",
    "ts-trueskill": "^3.1.0"
  }
}
